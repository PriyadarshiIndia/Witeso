---
/* Contest Rankings section */
---
<div class="bg-neutral-900 border border-green-500 p-4 rounded-lg shadow-md space-y-8">
    <h3 class="text-center font-semibold">Contest Rankings</h3>
    <hr class="my-2 border-green-500">
    <div class="space-y-8">
      <!-- LeetCode Ranking -->
      <div class="text-center">
        <h4 class="font-semibold">LEETCODE</h4>
        <div class="flex items-center justify-center gap-4 mt-4">
          <img src="https://assets.leetcode.com/static_assets/public/images/badges/guardian.png" alt="LeetCode Badge" class="w-24 h-24">
          <div>
            <h3 class="text-4xl font-bold">2185</h3>
            <p class="text-sm">(max : 2282)</p>
          </div>
        </div>
      </div>
      <!-- Codechef Ranking -->
      <div class="text-center">
        <h4 class="font-semibold">CODECHEF</h4>
        <div class="flex items-center justify-center mt-4">
          <div>
            <h3 class="text-4xl font-bold">2112</h3>
            <p class="text-sm">(max : 2224)</p>
          </div>
        </div>
      </div>
      <!-- CodeForces Ranking -->
      <div class="text-center">
        <h4 class="font-semibold">CODEFORCES</h4>
        <div class="flex items-center justify-center gap-4 mt-4">
          <img src="https://codolio.com/landing/codeforces_user_icon.png" alt="CodeForces Badge" class="w-24 h-24">
          <div>
            <h3 class="text-4xl font-bold">1689</h3>
            <p class="text-sm">(max : 1689)</p>
            <p class="text-sm">Expert</p>
          </div>
        </div>
      </div>
    </div>
  </div>
  